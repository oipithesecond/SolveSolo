{
    "manifest_version": 3,
    "name": "SolveSolo",
    "version": "1.0",
    "description": "Blocks AI sites while practicing DSA",
    "permissions": [
      "tabs",
      "storage",
      "alarms"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "SolveSolo"
    },
    "options_ui": {
      "page": "options.html",
      "open_in_tab": false
  }
  }